<div class="container">
  <div class="jumbotron">
    <h1>Students in class</h1>
</div>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">id</th>
      <th scope="col">password</th>
    </tr>
  </thead>
  <tbody>
{{#each usersArray}}
    <tr>
      <th scope="row"></th>
    <td>{{first_name}}</td>
    <td>{{last_name}}</td>
    <td>{{user_id}}</td>
    <td>{{password}}</td>
    </tr>
   
{{/each}}
  </tbody>
</table>
</br>
</br>
